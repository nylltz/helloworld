{
  "name": "notes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aws-amplify": "^1.1.6",
    "aws-amplify-react": "^2.0.7",
    "bootstrap": "^4.1.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4",
    "imagemagick": "0.1.3",
  },
  "scripts": {
    "start": "env-cmd -e development -r .env-cmdrc react-scripts start",
    "build_dev": "env-cmd -e development -r .env-cmdrc react-scripts --max_old_space_size=4096 build",
    "build_staging": "env-cmd -e test -r .env-cmdrc react-scripts --max_old_space_size=4096 build",
    "build_master": "env-cmd -e production -r .env-cmdrc react-scripts --max_old_space_size=4096 build",
    "build": "react-scripts build",
    "test": "env-cmd -e test -r .env-cmdrc react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
